{
    "name": "dhenfie/nuri",
    "description": "PHP Template System",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "dhenfie",
            "email": "fajarsusilo1600@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dhenfie\\Nuri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhenfie\\Nuri\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit",
        "cs-fix": "./vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix ./src --rules=@PSR12"
    }
}
